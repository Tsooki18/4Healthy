@model IEnumerable<_4Healthy_.Models.Alimento>
<div>
    <Strong>
        Alimento
    </Strong>
    <div class="panel panel-primary">
        <div class="panel-heading">
            Relação de Alimentos cadastrados
        </div>
        <div class="panel-body">
            <table class="table table-striped table-hover">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.Descricao)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Calorias)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Carboidratos)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Proteinas)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Gorduras)
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Descricao)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Calorias)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Carboidratos)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Proteinas)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Gorduras)
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
